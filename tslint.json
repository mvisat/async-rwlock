{
    "extends": [
        "tslint:latest",
        "tslint-config-prettier"
    ],
    "rules": {
        "interface-name": [true, "never-prefix"],
        "prefer-object-spread": false,
        "object-literal-sort-keys": false,
        "variable-name": ["check-format", "allow-leading-underscore"]
    }
}
